// Online C++ compiler to run C++ program online
#include <iostream>
#include<vector>
#include<unordered_map>
using namespace std;

int main() {
    
    string input="hello guys";

    unordered_map<char,int> count;
    for(int i:input){
        count[i]++;
    }
    for(const auto& pair:count){
        cout<<pair.first<<":"<<pair.second<<endl;
    }
}
