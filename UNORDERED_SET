/ Online C++ compiler to run C++ program online
#include <iostream>
#include<vector>
#include<unordered_map>
#include<unordered_set>
using namespace std;

int main() {
    
    string input="hello";

    unordered_set<char> count;
    for(int i:input){
        count.insert(i);
    }
    for(char i:count){
        cout<<i<<endl;
    }
}
